<Defs>
	<!-- ============================== GenericAmmo ==================================== -->
	<ThingCategoryDef>
		<defName>AmmoGeneric</defName>
		<label>Generic Ammo</label>
		<!--Maybe change this-->
		<parent>AmmoRifle</parent>
		<iconPath>UI/Icons/ThingCategories/CaliberRifle</iconPath>
	</ThingCategoryDef>
	<!-- ==================== AmmoSet ========================== -->
	<CombatExtended.AmmoSetDef>
		<defName>Generic_Ammo</defName>
		<label>Generic Ammo</label>
		<ammoTypes>
			<Ammo_Generic_FMJ>CEAP_Bullet_Generic_FMJ</Ammo_Generic_FMJ>
			<Ammo_Generic_AP>CEAP_Bullet_Generic_AP</Ammo_Generic_AP>
			<Ammo_Generic_HP>CEAP_Bullet_Generic_HP</Ammo_Generic_HP>
		</ammoTypes>
	</CombatExtended.AmmoSetDef>
	
	<!-- ==================== Ammo ========================== -->
	<ThingDef Class="CombatExtended.AmmoDef" Name="GenericAmmoBase" ParentName="SmallAmmoBase" Abstract="True">
		<description>Just your average, generic, killing-people ammo.</description>
		<statBases>
			<Mass>0.013</Mass>
			<Bulk>0.02</Bulk>
		</statBases>
		<tradeTags>
			<li>CE_AutoEnableTrade</li>
			<li>CE_AutoEnableCrafting</li>
		</tradeTags>
		<thingCategories>
			<li>AmmoGeneric</li>
		</thingCategories>
	</ThingDef>
	<ThingDef Class="CombatExtended.AmmoDef" ParentName="GenericAmmoBase">
		<defName>CEAP_Ammo_Generic_FMJ</defName>
		<label>Generic Ammo (FMJ)</label>
		<graphicData>
			<texPath>Things/Ammo/Rifle/FMJ</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>0.06</MarketValue>
		</statBases>
		<ammoClass>FullMetalJacket</ammoClass>
		<cookOffProjectile>Bullet_Generic_FMJ</cookOffProjectile>
	</ThingDef>
	<ThingDef Class="CombatExtended.AmmoDef" ParentName="GenericAmmoBase">
		<defName>CEAP_Ammo_Generic_AP</defName>
		<label>Generic Ammo (AP)</label>
		<graphicData>
			<texPath>Things/Ammo/Rifle/AP</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>0.06</MarketValue>
		</statBases>
		<ammoClass>ArmorPiercing</ammoClass>
		<cookOffProjectile>Bullet_Generic_AP</cookOffProjectile>
	</ThingDef>
	<ThingDef Class="CombatExtended.AmmoDef" ParentName="GenericAmmoBase">
		<defName>CEAP_Ammo_Generic_HP</defName>
		<label>Generic Ammo (HP)</label>
		<graphicData>
			<texPath>Things/Ammo/Rifle/HP</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>0.09</MarketValue>
		</statBases>
		<ammoClass>HollowPoint</ammoClass>
		<cookOffProjectile>Bullet_Generic_HP</cookOffProjectile>
	</ThingDef>
 
	<!-- ================== Projectiles ================== -->
	<ThingDef Name="GenericAmmoBullet" ParentName="BaseBullet" Abstract="true">
		<graphicData>
			<texPath>Projectile/Bullet_Small</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageDef>Bullet</damageDef>
			<speed>185</speed>
			<dropsCasings>true</dropsCasings>
		</projectile>
	</ThingDef>
	<ThingDef ParentName="GenericAmmoBullet">
		<defName>Bullet_Generic_FMJ</defName>
		<label>Generic bullet (FMJ)</label>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageAmountBase>1</damageAmountBase>
			<armorPenetrationSharp>1</armorPenetrationSharp>
			<armorPenetrationBlunt>1</armorPenetrationBlunt>
		</projectile>
	</ThingDef>
	<ThingDef ParentName="GenericAmmoBullet">
		<defName>Bullet_Generic_AP</defName>
		<label>Generic bullet (AP)</label>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageAmountBase>1</damageAmountBase>
			<armorPenetrationSharp>1</armorPenetrationSharp>
			<armorPenetrationBlunt>1</armorPenetrationBlunt>
		</projectile>
	</ThingDef>
	<ThingDef ParentName="GenericAmmoBullet">
		<defName>Bullet_Generic_HP</defName>
		<label>Generic bullet (HP)</label>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageAmountBase>1</damageAmountBase>
			<armorPenetrationSharp>1</armorPenetrationSharp>
			<armorPenetrationBlunt>1</armorPenetrationBlunt>
		</projectile>
	</ThingDef>
	<!-- ==================== Recipes ========================== -->

  <!-- Base for hand grenades -->
 
</Defs>
